{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/_bootstrap.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		},
		"_bootstrap": {
			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"Shot": {
			"type": "script",
			"path": "script/Shot.js",
			"global": true
		},
		"Player": {
			"type": "script",
			"path": "script/Player.js",
			"global": true
		},
		"IUtil": {
			"type": "script",
			"path": "script/IUtil.js",
			"global": true
		},
		"MTime": {
			"type": "script",
			"path": "script/MTime.js",
			"global": true
		},
		"MyScene": {
			"type": "script",
			"path": "script/MyScene.js",
			"global": true
		},
		"gameend": {
			"type": "image",
			"width": 640,
			"height": 340,
			"path": "image/gameend.png"
		},
		"title": {
			"type": "image",
			"width": 640,
			"height": 340,
			"path": "image/title.png"
		}
	},
	"moduleMainScripts": {},
	"environment": {
		"sandbox-runtime": "3",
		"niconico": {
			"supportedModes": [
				"ranking"
			]
		}
